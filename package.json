{
  "name": "@iosio/react-router",
  "version": "1.0.22",
  "description": "simple react router",
  "type": "module",
  "source": "src/index.js",
  "main": "dist/iosio-router.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/evatrium/react-router.git"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "react router",
    "simple routing",
    "pushState navigation"
  ],
  "scripts": {
    "start": "wmr",
    "build": "microbundle src/index.js -o dist/iosio-router.js --jsx React.createElement --no-pkg-main -f modern --sourcemap false --no-compress --define process.env.NODE_ENV=production --name iosioRouter",
    "test": "exit 0",
    "iosio": "npm uninstall @iosio/util && npm install @iosio/util"
  },
  "devDependencies": {
    "microbundle": "^0.14.1",
    "preact": "^10.5.15",
    "react": "^18.2.0",
    "wmr": "^3.7.2"
  },
  "dependencies": {
    "@iosio/util": "^2.0.57"
  }
}
